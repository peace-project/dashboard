<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:ti="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" name="SA00006-RethrowInIf" targetNamespace="http://dsg.wiai.uniba.de/betsy/rules/sa00006/bpel/SA00006-RethrowInIf">
    <import namespace="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" location="TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="InitData" messageType="ti:executeProcessSyncRequest"/>
    </variables>
    <sequence>
        <receive name="InitialReceive" createInstance="yes" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="InitData"/>
        <assign name="AssignFalseToReplyData">
            <copy>
                <from>0</from>
                <to variable="ReplyData" part="outputPart"/>
            </copy>
        </assign>
        <if name="TestIfInputIsEven">
            <condition>$InitData.inputPart mod 2 = 0</condition>
            <rethrow name="Rethrow"/>
        </if>
        <reply name="ReplyToInitialReceive" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="ReplyData"/>
    </sequence>
</process>
