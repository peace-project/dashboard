<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:ti="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" xmlns:xs="http://www.w3.org/2001/XMLSchema" name="SA00047-EmptyMessage-Receive-FromParts" targetNamespace="http://dsg.wiai.uniba.de/betsy/rules/sa00047/bpel/SA00047-EmptyMessage-Receive-FromParts">
    <import namespace="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" location="TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="TempInt" type="xs:int"/>
    </variables>
    <sequence>
        <receive name="InitialReceive" partnerLink="MyRoleLink" operation="startProcessSyncWithEmptyMessage" portType="ti:TestInterfacePortType" createInstance="yes">
            <fromParts>
                <fromPart part="inputPart" toVariable="TempInt"/>
            </fromParts>
        </receive>
        <assign>
            <copy>
                <from variable="TempInt"/>
                <to variable="ReplyData" part="outputPart"/>
            </copy>
        </assign>
        <reply name="CorrelatedReply" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="ReplyData"/>
    </sequence>
</process>
