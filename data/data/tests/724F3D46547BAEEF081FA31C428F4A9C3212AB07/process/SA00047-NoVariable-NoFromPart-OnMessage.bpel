<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:ti="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" xmlns:xs="http://www.w3.org/2001/XMLSchema" name="SA00047-NoVariable-NoFromPart-OnMessage" targetNamespace="http://dsg.wiai.uniba.de/betsy/rules/sa00047/bpel/SA00047-NoVariable-NoFromPart-OnMessage">
    <import namespace="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" location="TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="TempInt" type="xs:int"/>
    </variables>
    <sequence>
        <pick name="Pick" createInstance="yes">
            <onMessage partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType">
                <sequence name="Sequence">
                    <assign>
                        <copy>
                            <from variable="TempInt"/>
                            <to variable="ReplyData" part="outputPart"/>
                        </copy>
                    </assign>
                    <reply name="ReplyToInitialReceive" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="ReplyData"/>
                </sequence>
            </onMessage>
        </pick>
    </sequence>
</process>
