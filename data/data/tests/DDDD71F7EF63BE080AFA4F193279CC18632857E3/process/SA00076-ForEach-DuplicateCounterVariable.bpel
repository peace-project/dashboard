<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ti="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" name="SA00076-ForEach-DuplicateCounterVariable" targetNamespace="http://dsg.wiai.uniba.de/betsy/rules/sa00076/bpel/SA00076-ForEach-DuplicateCounterVariable">
    <import namespace="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" location="TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="InitData" messageType="ti:executeProcessSyncRequest"/>
    </variables>
    <sequence>
        <receive name="InitialReceive" createInstance="yes" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="InitData"/>
        <assign name="InitializeReplyDataToZero">
            <copy>
                <from>0</from>
                <to variable="ReplyData" part="outputPart"/>
            </copy>
        </assign>
        <forEach name="ForEach" parallel="no" counterName="ForEachCounter">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>$InitData.inputPart</finalCounterValue>
            <scope name="Scope1">
                <variables>
                    <variable name="ForEachCounter" type="xsd:string"/>
                </variables>
                <assign name="AddTurnNumberToReplyData">
                    <copy>
                        <from>$ReplyData.outputPart + $ForEachCounter</from>
                        <to variable="ReplyData" part="outputPart"/>
                    </copy>
                </assign>
            </scope>
        </forEach>
        <reply name="ReplyToInitialReceive" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="ReplyData"/>
    </sequence>
</process>
