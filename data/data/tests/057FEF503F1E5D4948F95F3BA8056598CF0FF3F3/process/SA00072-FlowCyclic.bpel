<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:ti="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="SA00072-FlowCyclic" targetNamespace="http://dsg.wiai.uniba.de/betsy/rules/sa00072/bpel/SA00072-FlowCyclic">
    <import namespace="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" location="TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="InitData" messageType="ti:executeProcessSyncRequest"/>
        <variable name="BranchVariable" type="xsd:int"/>
    </variables>
    <sequence>
        <receive name="InitialReceive" createInstance="yes" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="InitData"/>
        <flow name="Flow">
            <links>
                <link name="FromFirstToSecond"/>
                <link name="andBackAgain"/>
            </links>
            <assign name="SetBranch2">
                <targets>
                    <target linkName="FromFirstToSecond"/>
                </targets>
                <sources>
                    <source linkName="andBackAgain"/>
                </sources>
                <copy>
                    <from>2</from>
                    <to variable="BranchVariable"/>
                </copy>
            </assign>
            <assign name="SetBranch1">
                <targets>
                    <target linkName="andBackAgain"/>
                </targets>
                <sources>
                    <source linkName="FromFirstToSecond"/>
                </sources>
                <copy>
                    <from>1</from>
                    <to variable="BranchVariable"/>
                </copy>
            </assign>
        </flow>
        <assign name="AssignReplyData">
            <copy>
                <from>$BranchVariable</from>
                <to variable="ReplyData" part="outputPart"/>
            </copy>
        </assign>
        <reply name="ReplyToInitialReceive" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="ReplyData"/>
    </sequence>
</process>
