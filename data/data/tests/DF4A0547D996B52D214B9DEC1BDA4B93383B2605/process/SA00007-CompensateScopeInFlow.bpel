<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:ti="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="SA00007-CompensateScopeInFlow" targetNamespace="http://dsg.wiai.uniba.de/betsy/rules/sa00007/bpel/SA00007-CompensateScopeInFlow">
    <import namespace="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" location="TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="InitData" messageType="ti:executeProcessSyncRequest"/>
        <variable name="Branch1" type="xsd:int"/>
        <variable name="Branch2" type="xsd:int"/>
    </variables>
    <sequence>
        <receive name="InitialReceive" createInstance="yes" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="InitData"/>
        <flow name="Flow">
            <assign name="SetBranch1">
                <copy>
                    <from>1</from>
                    <to variable="Branch1"/>
                </copy>
            </assign>
            <assign name="SetBranch2">
                <copy>
                    <from>1</from>
                    <to variable="Branch2"/>
                </copy>
            </assign>
            <compensateScope name="CompensateScope" target="compensationScope"/>
        </flow>
        <assign name="AssignReplyData">
            <copy>
                <from>$Branch1 + $InitData.inputPart + $Branch2</from>
                <to variable="ReplyData" part="outputPart"/>
            </copy>
        </assign>
        <scope name="compensationScope"><compensationHandler><empty/></compensationHandler><empty/></scope>
        <reply name="ReplyToInitialReceive" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="ReplyData"/>
    </sequence>
</process>
