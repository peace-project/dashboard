<?xml version="1.0" encoding="UTF-8"?>
<bpmn2:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/20100501/BPMN20.xsd"
                   id="Escalation_IntermediateThrowEventTest"
                   targetNamespace="http://dsg.wiai.uniba.de/betsy/bpmn/escalation_IntermediateThrowEvent">
    <bpmn2:escalation id="ESC_1" name="Escalation1" escalationCode="ESC_1"/>
    <bpmn2:process id="Escalation_IntermediateThrowEvent" isExecutable="true">
        <bpmn2:startEvent id="StartEvent_1">
            <bpmn2:outgoing>SequenceFlow_1</bpmn2:outgoing>
        </bpmn2:startEvent>

        <bpmn2:sequenceFlow id="SequenceFlow_1" name="" sourceRef="StartEvent_1" targetRef="start"/>

        <bpmn2:scriptTask id="start" name="Start">
            <bpmn2:incoming>SequenceFlow_1</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing>
            <bpmn2:script>CREATE_LOG_FILE</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_2" name="" sourceRef="start" targetRef="SubProcess_1"/>

        <bpmn2:subProcess id="SubProcess_1">
            <bpmn2:incoming>SequenceFlow_2</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_3</bpmn2:outgoing>

            <bpmn2:startEvent id="StartEvent_2">
                <bpmn2:outgoing>SequenceFlow_5</bpmn2:outgoing>
            </bpmn2:startEvent>

            <bpmn2:sequenceFlow id="SequenceFlow_5" name="" sourceRef="StartEvent_2" targetRef="IntermediateEvent_1"/>

            <bpmn2:intermediateThrowEvent id="IntermediateEvent_1" name="Intermediate Event">
                <bpmn2:incoming>SequenceFlow_5</bpmn2:incoming>
                <bpmn2:outgoing>SequenceFlow_7</bpmn2:outgoing>
                <bpmn2:escalationEventDefinition escalationRef="ESC_1" id="EscEndDef"/>
            </bpmn2:intermediateThrowEvent>

            <bpmn2:sequenceFlow id="SequenceFlow_7" name="" sourceRef="IntermediateEvent_1" targetRef="EndEvent_3"/>

            <bpmn2:endEvent id="EndEvent_3">
                <bpmn2:incoming>SequenceFlow_7</bpmn2:incoming>
            </bpmn2:endEvent>
        </bpmn2:subProcess>

        <bpmn2:boundaryEvent id="BoundaryEvent_1" name="" attachedToRef="SubProcess_1" cancelActivity="false">
            <bpmn2:outgoing>SequenceFlow_9</bpmn2:outgoing>
            <bpmn2:escalationEventDefinition escalationRef="ESC_1" id="EscBoundaryDef"/>
        </bpmn2:boundaryEvent>

        <bpmn2:sequenceFlow id="SequenceFlow_9" name="" sourceRef="BoundaryEvent_1" targetRef="ScriptTask_1"/>

        <bpmn2:scriptTask id="ScriptTask_1" name="Task1">
            <bpmn2:incoming>SequenceFlow_9</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_10</bpmn2:outgoing>
            <bpmn2:script>SCRIPT_task1</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_10" name="" sourceRef="ScriptTask_1" targetRef="EndEvent_2"/>

        <bpmn2:endEvent id="EndEvent_2">
            <bpmn2:incoming>SequenceFlow_10</bpmn2:incoming>
        </bpmn2:endEvent>

        <bpmn2:sequenceFlow id="SequenceFlow_3" name="" sourceRef="SubProcess_1" targetRef="EndEvent_1"/>

        <bpmn2:endEvent id="EndEvent_1">
            <bpmn2:incoming>SequenceFlow_3</bpmn2:incoming>
        </bpmn2:endEvent>
    </bpmn2:process>
</bpmn2:definitions>