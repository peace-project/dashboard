<?xml version="1.0" encoding="UTF-8"?>
<bpmn2:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/20100501/BPMN20.xsd"
                   id="Timer_BoundaryEvent_SubProcess_TimeCycleTest"
                   targetNamespace="http://dsg.wiai.uniba.de/betsy/bpmn/timer_BoundaryEvent_SubProcess_TimeCycle">
    <bpmn2:process id="Timer_BoundaryEvent_SubProcess_TimeCycle" isExecutable="true">

        <bpmn2:startEvent id="StartEvent_1" name="Start">
            <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing>
        </bpmn2:startEvent>

        <bpmn2:sequenceFlow id="SequenceFlow_2" name="" sourceRef="StartEvent_1" targetRef="start"/>

        <bpmn2:scriptTask id="start" name="Start">
            <bpmn2:incoming>SequenceFlow_2</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing>
            <bpmn2:script>CREATE_LOG_FILE</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_4" name="" sourceRef="start" targetRef="SubProcess"/>


        <bpmn2:subProcess id="SubProcess">
            <bpmn2:incoming>SequenceFlow_4</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_5</bpmn2:outgoing>

            <bpmn2:startEvent id="StartEvent_2">
                <bpmn2:outgoing>SequenceFlow_7</bpmn2:outgoing>
            </bpmn2:startEvent>

            <bpmn2:sequenceFlow id="SequenceFlow_7" name="" sourceRef="StartEvent_2"
                                targetRef="IntermediateCatchEvent"/>

            <bpmn2:intermediateCatchEvent id="IntermediateCatchEvent">
                <bpmn2:incoming>SequenceFlow_7</bpmn2:incoming>
                <bpmn2:outgoing>SequenceFlow_8</bpmn2:outgoing>
                <bpmn2:timerEventDefinition id="_TimerEventDefinition_4">
                    <bpmn2:timeDuration xsi:type="bpmn2:tFormalExpression">PT60S</bpmn2:timeDuration>
                </bpmn2:timerEventDefinition>
            </bpmn2:intermediateCatchEvent>

            <bpmn2:sequenceFlow id="SequenceFlow_8" name="" sourceRef="IntermediateCatchEvent"
                                targetRef="ScriptTask_2"/>

            <bpmn2:scriptTask id="ScriptTask_2" name="Task1">
                <bpmn2:incoming>SequenceFlow_8</bpmn2:incoming>
                <bpmn2:outgoing>SequenceFlow_9</bpmn2:outgoing>
                <bpmn2:script>SCRIPT_task1</bpmn2:script>
            </bpmn2:scriptTask>

            <bpmn2:sequenceFlow id="SequenceFlow_9" name="" sourceRef="ScriptTask_2" targetRef="EndEvent_1"/>

            <bpmn2:endEvent id="EndEvent_1">
                <bpmn2:incoming>SequenceFlow_9</bpmn2:incoming>
            </bpmn2:endEvent>

        </bpmn2:subProcess>


        <bpmn2:sequenceFlow id="SequenceFlow_5" name="" sourceRef="SubProcess" targetRef="end"/>

        <bpmn2:scriptTask id="end" name="Task2">
            <bpmn2:incoming>SequenceFlow_5</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_10</bpmn2:outgoing>
            <bpmn2:script>SCRIPT_task2</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_10" name="" sourceRef="end" targetRef="EndEvent_2"/>

        <bpmn2:endEvent id="EndEvent_2">
            <bpmn2:incoming>SequenceFlow_10</bpmn2:incoming>
        </bpmn2:endEvent>


        <bpmn2:boundaryEvent id="BoundaryEvent_1" name="" attachedToRef="SubProcess" cancelActivity="false">
            <bpmn2:outgoing>SequenceFlow_11</bpmn2:outgoing>
            <bpmn2:timerEventDefinition id="_TimerEventDefinition5">
                <bpmn2:timeCycle xsi:type="bpmn2:tFormalExpression">R4/PT10S</bpmn2:timeCycle>
            </bpmn2:timerEventDefinition>
        </bpmn2:boundaryEvent>

        <bpmn2:sequenceFlow id="SequenceFlow_11" name="" sourceRef="BoundaryEvent_1" targetRef="ScriptTask_3"/>

        <bpmn2:scriptTask id="ScriptTask_3" name="Task3">
            <bpmn2:incoming>SequenceFlow_11</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_12</bpmn2:outgoing>
            <bpmn2:script>SCRIPT_task3</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_12" name="" sourceRef="ScriptTask_3" targetRef="EndEvent_3"/>

        <bpmn2:endEvent id="EndEvent_3">
            <bpmn2:incoming>SequenceFlow_12</bpmn2:incoming>
        </bpmn2:endEvent>

    </bpmn2:process>
</bpmn2:definitions>