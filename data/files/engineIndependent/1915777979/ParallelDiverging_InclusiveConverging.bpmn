<?xml version="1.0" encoding="UTF-8"?>
<bpmn2:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/20100501/BPMN20.xsd"
                   id="ParallelDiverging_InclusiveConvergingTest"
                   targetNamespace="http://dsg.wiai.uniba.de/betsy/bpmn/parallelDiverging_InclusiveConverging">
    <bpmn2:process id="ParallelDiverging_InclusiveConverging" isExecutable="true">

        <bpmn2:startEvent id="StartEvent">
            <bpmn2:outgoing>SequenceFlow_1</bpmn2:outgoing>
        </bpmn2:startEvent>

        <bpmn2:sequenceFlow id="SequenceFlow_1" name="" sourceRef="StartEvent" targetRef="start"/>

        <bpmn2:scriptTask id="start" name="Start">
            <bpmn2:incoming>SequenceFlow_1</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing>
            <bpmn2:script>CREATE_LOG_FILE</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_2" name="" sourceRef="start" targetRef="ParallelGateway_1"/>

        <bpmn2:parallelGateway id="ParallelGateway_1" gatewayDirection="Diverging">
            <bpmn2:incoming>SequenceFlow_2</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_3</bpmn2:outgoing>
            <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing>
        </bpmn2:parallelGateway>


        <bpmn2:sequenceFlow id="SequenceFlow_3" name="" sourceRef="ParallelGateway_1" targetRef="task1"/>

        <bpmn2:scriptTask id="task1" name="Task 1">
            <bpmn2:incoming>SequenceFlow_3</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_6</bpmn2:outgoing>
            <bpmn2:script>SCRIPT_task1</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_6" name="" sourceRef="task1" targetRef="InclusiveGateway_2"/>


        <bpmn2:sequenceFlow id="SequenceFlow_4" name="" sourceRef="ParallelGateway_1" targetRef="task2"/>

        <bpmn2:scriptTask id="task2" name="Task 2">
            <bpmn2:incoming>SequenceFlow_4</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_5</bpmn2:outgoing>
            <bpmn2:script>SCRIPT_task2</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_5" name="" sourceRef="task2" targetRef="InclusiveGateway_2"/>


        <bpmn2:inclusiveGateway id="InclusiveGateway_2" gatewayDirection="Converging">
            <bpmn2:incoming>SequenceFlow_5</bpmn2:incoming>
            <bpmn2:incoming>SequenceFlow_6</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_7</bpmn2:outgoing>
        </bpmn2:inclusiveGateway>

        <bpmn2:sequenceFlow id="SequenceFlow_7" name="" sourceRef="InclusiveGateway_2" targetRef="end"/>

        <bpmn2:scriptTask id="end" name="End">
            <bpmn2:incoming>SequenceFlow_7</bpmn2:incoming>
            <bpmn2:outgoing>SequenceFlow_8</bpmn2:outgoing>
            <bpmn2:script>SCRIPT_task3</bpmn2:script>
        </bpmn2:scriptTask>

        <bpmn2:sequenceFlow id="SequenceFlow_8" name="" sourceRef="end" targetRef="EndEvent"/>

        <bpmn2:endEvent id="EndEvent">
            <bpmn2:incoming>SequenceFlow_8</bpmn2:incoming>
        </bpmn2:endEvent>

    </bpmn2:process>
</bpmn2:definitions>