<bpmn2:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/20100501/BPMN20.xsd"
                   id="Signal_EndEventTest"
                   targetNamespace="http://dsg.wiai.uniba.de/betsy/bpmn/signal_EndEvent">
   <bpmn2:signal id="signal" name="Signal"/>
   <bpmn2:process id="Signal_EndEvent" isExecutable="true">
      <bpmn2:startEvent id="StartEvent1">
         <bpmn2:outgoing>SequenceFlow_3</bpmn2:outgoing>
      </bpmn2:startEvent>
      <bpmn2:sequenceFlow id="SequenceFlow_3" name="" sourceRef="StartEvent1" targetRef="start"/>
      <bpmn2:scriptTask scriptFormat="groovy" id="start" name="Start">
         <bpmn2:incoming>SequenceFlow_3</bpmn2:incoming>
         <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing><bpmn2:script>
try {
    java.io.File f = new java.io.File("log" + testCaseNumber + ".txt");
    f.createNewFile();
} catch (java.io.IOException ignore) {}
</bpmn2:script></bpmn2:scriptTask>
      <bpmn2:sequenceFlow id="SequenceFlow_4" name="" sourceRef="start" targetRef="task1"/>
      <bpmn2:scriptTask scriptFormat="groovy" id="task1" name="Task 1">
         <bpmn2:incoming>SequenceFlow_4</bpmn2:incoming>
         <bpmn2:outgoing>SequenceFlow_5</bpmn2:outgoing><bpmn2:script>
java.io.BufferedWriter bw = new java.io.BufferedWriter(new java.io.FileWriter("log" + testCaseNumber + ".txt", true));
try{
    bw.append("SCRIPT_task1");
    bw.newLine();
} catch (java.io.IOException ignored){
} finally {
    if (bw != null) {
        try {
            bw.close();
        } catch (java.io.IOException ignored){}
    }
}
</bpmn2:script></bpmn2:scriptTask>
      <bpmn2:sequenceFlow id="SequenceFlow_5" name="" sourceRef="task1" targetRef="EndEvent_1"/>
      <bpmn2:endEvent id="EndEvent_1">
         <bpmn2:incoming>SequenceFlow_5</bpmn2:incoming>
         <bpmn2:signalEventDefinition id="_SignalEventDefinition_1" signalRef="signal"/>
      </bpmn2:endEvent>
   </bpmn2:process>
   <bpmn2:process id="CalledProcess" isExecutable="true">
      <bpmn2:startEvent id="StartEvent2" name="Start">
         <bpmn2:outgoing>SequenceFlow_1</bpmn2:outgoing>
         <bpmn2:signalEventDefinition id="_SignalEventDefinition_2" signalRef="signal"/>
      </bpmn2:startEvent>
      <bpmn2:sequenceFlow id="SequenceFlow_1" name="" sourceRef="StartEvent2" targetRef="createMarker"/>
      <bpmn2:scriptTask scriptFormat="groovy" id="createMarker" name="Start">
         <bpmn2:incoming>SequenceFlow_1</bpmn2:incoming>
         <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing><bpmn2:script>
try {
    java.io.File f = new java.io.File("MARKER");
    f.createNewFile();
} catch (java.io.IOException ignore) {}
</bpmn2:script></bpmn2:scriptTask>
      <bpmn2:sequenceFlow id="SequenceFlow_2" name="" sourceRef="createMarker" targetRef="EndEvent_2"/>
      <bpmn2:endEvent id="EndEvent_2">
         <bpmn2:incoming>SequenceFlow_2</bpmn2:incoming>
      </bpmn2:endEvent>
   </bpmn2:process>
   <bpmn2:collaboration id="collaboration" isClosed="false">
      <bpmn2:participant id="Signal_EndEvent_Participant" name="Signal_EndEvent_Participant"
                         processRef="Signal_EndEvent">
         <bpmn2:participantMultiplicity maximum="1" minimum="0"/>
      </bpmn2:participant>
      <bpmn2:participant id="CalledProcess_Participant" name="CalledProcess_Participant"
                         processRef="CalledProcess">
         <bpmn2:participantMultiplicity maximum="1" minimum="0"/>
      </bpmn2:participant>
   </bpmn2:collaboration>
</bpmn2:definitions>