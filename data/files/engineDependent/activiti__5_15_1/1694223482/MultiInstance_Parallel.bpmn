                   xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/20100501/BPMN20.xsd"
                   id="MultiInstance_ParallelTest"
                   targetNamespace="http://dsg.wiai.uniba.de/betsy/bpmn/multiInstance_Parallel">
   <bpmn2:process id="MultiInstance_Parallel" isExecutable="true">
      <bpmn2:startEvent id="StartEvent" name="Start">
         <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing>
      </bpmn2:startEvent>
      <bpmn2:sequenceFlow id="SequenceFlow_2" name="" sourceRef="StartEvent" targetRef="start"/>
      <bpmn2:scriptTask scriptFormat="groovy" id="start" name="Start">
         <bpmn2:incoming>SequenceFlow_2</bpmn2:incoming>
         <bpmn2:outgoing>SequenceFlow_1</bpmn2:outgoing><bpmn2:script>
try {
    java.io.File f = new java.io.File("log" + testCaseNumber + ".txt");
    f.createNewFile();
} catch (java.io.IOException ignore) {}
</bpmn2:script></bpmn2:scriptTask>
      <bpmn2:sequenceFlow id="SequenceFlow_1" name="" sourceRef="start" targetRef="MultiInstanceTask"/>
      <bpmn2:scriptTask scriptFormat="groovy" id="MultiInstanceTask" name="MultiInstanceTask">
         <bpmn2:incoming>SequenceFlow_1</bpmn2:incoming>
         <bpmn2:outgoing>SequenceFlow_5</bpmn2:outgoing>
         <bpmn2:multiInstanceLoopCharacteristics isSequential="false">
            <bpmn2:loopCardinality xsi:type="bpmn2:tFormalExpression">3</bpmn2:loopCardinality>
         </bpmn2:multiInstanceLoopCharacteristics><bpmn2:script>
java.io.BufferedWriter bw = new java.io.BufferedWriter(new java.io.FileWriter("log" + testCaseNumber + ".txt", true));
try{
    bw.append("SCRIPT_task1");
    bw.newLine();
} catch (java.io.IOException ignored){
} finally {
    if (bw != null) {
        try {
            bw.close();
        } catch (java.io.IOException ignored){}
    }
}
</bpmn2:script></bpmn2:scriptTask>
      <bpmn2:sequenceFlow id="SequenceFlow_5" name="" sourceRef="MultiInstanceTask" targetRef="end"/>
      <bpmn2:scriptTask scriptFormat="groovy" id="end" name="End">
         <bpmn2:incoming>SequenceFlow_5</bpmn2:incoming>
         <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing><bpmn2:script>
java.io.BufferedWriter bw = new java.io.BufferedWriter(new java.io.FileWriter("log" + testCaseNumber + ".txt", true));
try{
    bw.append("SCRIPT_task2");
    bw.newLine();
} catch (java.io.IOException ignored){
} finally {
    if (bw != null) {
        try {
            bw.close();
        } catch (java.io.IOException ignored){}
    }
}
</bpmn2:script></bpmn2:scriptTask>
      <bpmn2:sequenceFlow id="SequenceFlow_4" name="" sourceRef="end" targetRef="EndEvent"/>
      <bpmn2:endEvent id="EndEvent">
         <bpmn2:incoming>SequenceFlow_4</bpmn2:incoming>
      </bpmn2:endEvent>
   </bpmn2:process>
</bpmn2:definitions>